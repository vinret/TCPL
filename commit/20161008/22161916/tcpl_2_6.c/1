#include<stdio.h>
int main()
{
    unsigned x;
    int p;
    int n;
    unsigned y;
    printf("x=");
    scanf("%o",&x);
    printf("p=");
    scanf("%d,&p);
    printf("n=");
    scanf("%d",&n);
    printf("y=");
    scanf("%o",&y);
    (x&((~o<<(p+1))|(~(~o<<(p+1-n))))|((y&~(~o<<n))<<(p+1-n));
    printf("x=%",x);
    return 0;
}
