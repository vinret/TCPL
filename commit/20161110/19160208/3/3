#include <stdio.h>
void print_hex(int a)
int main()
{
  int a;
  scanf("%d",&a); }
  print_hex(a);
  return 0;
} 
{
    int i;
    char c[10];
    for(i=0;a!=0;++i)
    {
        if(a%16>=0&&a%16<10)
        {
            c[i]=(char)(a%16)+48;
        }
        else
             c[i]=(char)(a%16)+55;
             a=a/16;
     }
     for(--i;i>=0;--i) printf("%c",c[i]);
     printf("\n")
}
